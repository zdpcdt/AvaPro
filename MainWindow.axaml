<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="Window with Button"
        mc:Ignorable="d" d:DesignWidth="500" d:DesignHeight="200"
        x:Class="AvaPro.MainWindow"
        >
        <StackPanel Margin="10">
                <Button Content="{Binding BlockText}" />
                <Button Content="{Binding StartServerText}" Command="{Binding StartServerButtonClicked}"/>
                <Button Content="{Binding SendMsgText}" Command="{Binding SendMsgButtonClicked}"/>
                <!-- <Button Content="{Binding ButtonText}" Command="{Binding SendMsgButtonClicked}"/> -->
        </StackPanel>
</Window>
